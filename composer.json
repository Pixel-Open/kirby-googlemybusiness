{
  "name": "pixelopen/kirby-googlemybusiness",
  "type": "kirby-plugin",
  "description": "A plugin for Kirby CMS to handle Google My Business Website Profile",
  "homepage": "https://github.com/Pixel-Open/kirby-googlemybusiness",
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel Open",
      "homepage": "https://pixel-open.org/"
    }
  ],
  "keywords": [
    "kirby",
    "kirby-cms",
    "kirby-plugin"
  ],
  "require": {
    "php": ">=8.2.0",
    "getkirby/composer-installer": "^1.2",
    "getkirby/cms": "^4.0 || ^5.0",
    "getkirby/cli": "^1.1"
  },
  "extra": {
    "installer-name": "kirby-googlemybusiness"
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^12.0"
  }
}
